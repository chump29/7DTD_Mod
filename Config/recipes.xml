<chump>
	<!-- Gas stove -->
	<append xpath="/recipes">
		<recipe name="gasStove" count="1" craft_area="workbench">
			<ingredient name="resourceForgedIron" count="10" />
			<ingredient name="resourceMetalPipe" count="2" />
			<ingredient name="resourceMechanicalParts" count="5" />
			<ingredient name="resourceElectricParts" count="7" />
		</recipe>
	</append>
	<!-- Animal snare -->
	<append xpath="/recipes">
		<recipe name="animalFeed" count="1" craftTime="3" scrapable="False">
			<ingredient name="foodCropPumpkin" count="3" />
		</recipe>
		<recipe name="animalFeed" count="1" craftTime="3" scrapable="False">
			<ingredient name="foodCropCorn" count="1" />
		</recipe>
		<recipe name="animalSnare" count="1" craftTime="60" scrapable="False">
			<ingredient name="resourceWood" count="5" />
			<ingredient name="resourceYuccaFibers" count="6" />
			<ingredient name="animalFeed" count="2" />
		</recipe>
	</append>
	<!-- Charcoal -->
	<append xpath="/recipes">
		<recipe name="charcoal" count="10" craftTime="10" scrapable="False">
			<ingredient name="resourceWood" count="20" />
		</recipe>
	</append>
</chump>